{
  "trailingComma": "es5",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^(react/(.*)$)|^(react$))",
    "^(hono/(.*)$)|^(hono$))",
    "^(drizzle-orm/(.*)$)|^(drizzle-orm$))",
    "^(@tanstack/react-router/(.*)$)|^(@tanstack/react-router$)/)",
    "^(@tanstack/(.*)$)|^(@tanstack$)",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "^@/env(.*)$",
    "^@/types/(.*)$",
    "^@/shared/(.*)$",
    "^@/lib(.*)$",
    "^@/hooks/(.*)$",
    "",
    "^@/features/(.*)/schema(.*)$",
    "^@/features/(.*)/types(.*)$",
    "^@/features/(.*)/api(.*)$",
    "^@/features/(.*)/hooks(.*)$",
    "^@/features/(.*)/components(.*)$",
    "^@/features/(.*)$",
    "",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ]
}